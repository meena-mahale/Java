package Exception;
import java.util.ArrayList;
public class ArrList 
{ 
	public static void main(String[] args) 
	{ 
		ArrayList<Integer> myNumbers = new ArrayList<Integer>();
		
		myNumbers.add(10);
		myNumbers.add(15);
		myNumbers.add(20);
		myNumbers.add(25);
		
		for (int i : myNumbers) 
		{
			System.out.println(i);
		}
		
	
		for (int id = 0;id < myNumbers.size();id++) 
		{
			System.out.println(myNumbers.get(id));
		}
	} 
}
